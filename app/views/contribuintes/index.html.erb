<table class="tabela-contribuinte">
  <% @contribuintes.each do |contribuinte| %>
      <tr class="tr-contribuinte" url='/gastos/<%= contribuinte.id %>'>
        <td><%= contribuinte.nome %></td>
        <td> R$ 1000,00 </td>
      </tr>
  <% end %>
  <tr>
    <td> <b>Total</b> </td>
    <td> <b>R$ 2000,00</b> </td>
  </tr>
</table>
